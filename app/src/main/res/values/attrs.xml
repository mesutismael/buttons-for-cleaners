<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="typeface" format="enum">
        <enum name="regular" value="1"/>
        <enum name="medium" value="2"/>
    </attr>

    <declare-styleable name="TextView">
        <attr name="typeface"/>
    </declare-styleable>

    <declare-styleable name="Button">
        <attr name="typeface"/>
    </declare-styleable>

    <declare-styleable name="EditText">
        <attr name="typeface"/>
    </declare-styleable>

    <declare-styleable name="CheckBox">
        <attr name="typeface"/>
    </declare-styleable>

    <declare-styleable name="RadioButton">
        <attr name="typeface"/>
    </declare-styleable>

    <declare-styleable name="BoundedLinearLayout">
        <attr name="boundedWidth" format="dimension"/>
        <attr name="boundedHeight" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="RadiusCirclePageIndicator">
        <attr name="pageRadius" format="dimension"/>
    </declare-styleable>

</resources>